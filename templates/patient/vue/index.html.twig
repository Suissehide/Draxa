                        {% if atelier.categorie == 'Atelier' %}
                            {% set counter = (counter | default(0)) + 1 %}
                            <li data-date="{{ atelier.date|date("d/m/Y") }}" data-id="{{ atelier.id }}">
                                <h2>Atelier  
                                    {{ counter }}
                                </h2>
